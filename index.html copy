    <script>
        position = 0;

        function scrolltitle() {
            title = "Ilham Setia Bhakti Portfolio | ";
            document.title = title.substring(position, title.length) + title.substring(0, position);
            position++;
            if (position > title.length) position = 0;
            titleScroll = window.setTimeout(scrolltitle, 170);
        }
        scrolltitle();
      
      
      
      
fetch("https://api.rss2json.com/v1/api.json?rss_url=https://medium.com/feed/@isetiabhakti")
  .then((res) => res.json())
  .then((data) => {
    const res = data.items;
    console.log(res.length);
    var i;
    for (i = 0; i < res.length; i++) {
      console.log(res[i].title);
      $("#demo").append("<p><a target='_blank' href='"+ res[i].link + "'>" + res[i].title +"</a></p>");
    }
  });

fetch("https://api.rss2json.com/v1/api.json?rss_url=https://isetiabhakti.blogspot.com/feeds/posts/default")
  .then((res) => res.json())
  .then((data) => {
    const res = data.items;
    console.log(res.length);
    var i;
    for (i = 0; i < res.length; i++) {
      console.log(res[i].title);
      $("#demo").append("<p><a target='_blank' href='"+ res[i].link + "'>" + res[i].title +"</a></p>");
    }
  });

      
</script>

